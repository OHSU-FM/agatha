<div class="well">
  <% if pquestion.response_set.data[0].data.count < 4 and !@pk.to_s.empty? %>

    <table class="table table-striped table-condensed">
      <% pquestion.response_set.data.each do |rs| %>
        <% if !rs.data.first.nil? %>
          <tr>
            <td><span class="desc_<%= rs.qtype %>"><%= strip_tags(rs.question.question) %></span></td>
            <% rs.data.each do |dt| %>
              <% if dt.include? "^" %>
                <% tmp_dt = dt.split("^") %>
                <td><%= tmp_dt[0] %>
                <hr><b><i><%= tmp_dt[1] %></i></b>
                </td>
              <% else %>
                <td><%= dt %></td>
              <% end %>
            <% end %>
          </tr>
        <% end %>
      <% end %>
    </table>
  <% end %>
</div>
